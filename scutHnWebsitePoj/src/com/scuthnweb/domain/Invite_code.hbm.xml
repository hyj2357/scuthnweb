<?xml version="1.0" encoding="gb2312"?>
<!-- 指定Hiberante3映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--上面四行对所有的hibernate映射文件都相同 -->

<!-- hibernate-mapping是映射文件的根元素 -->
<hibernate-mapping package="com.scuthnweb.domain">
   <class name="Invite_code" table="invite_code">
      <id name="invite_code_id" type="int"><generator class="identity"/></id>
      <property name="invite_code_content" type="string" length="10"/>
    </class>
   
</hibernate-mapping>